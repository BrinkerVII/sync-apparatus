{
  "author": "BrinkerVII",
  "name": "sync-apparatus",
  "description": "A neat program that syncs your local filesystem to ROBLOX studio",
  "repository": "https://github.com/BrinkerVII/sync-apparatus",
  "version": "0.1.0",
  "license": "BSD-3-Clause",
  "main": "electron-app/main.js",
  "scripts": {
    "ng": "ng",
    "start": "webpack-dev-server --port=4200",
    "dev-server": "webpack-dev-server --port=4200 --colors",
    "build": "webpack",
    "test": "karma start ./karma.conf.js",
    "lint": "ng lint",
    "e2e": "protractor ./protractor.conf.js",
    "prepree2e": "npm start",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
    "electron": "electron ./electron-app",
    "electron:dev": "bnr electron:dev",
    "electron:build": "bnr electron:build",
    "pack": "bnr pack"
  },
  "betterScripts": {
    "electron:dev": {
      "command": "concurrently --kill-others \"npm run electron\" \"npm run dev-server\"",
      "env": {
        "DEBUG_ENABLED": true
      }
    },
    "electron:build": {
      "command": "webpack && electron ./electron-app",
      "env": {
        "ELECTRON_BUILD": true
      }
    },
    "pack": {
      "command": "gulp pack-plugin && build && build --dir",
      "env": {
        "ELECTRON_DIST": true
      }
    }
  },
  "build": {
    "appId": "com.roblox.brinkervii.sync-apparatus",
    "files": [
      "electron-app",
      "node_modules"
    ],
	"linux": {
		"target": "AppImage"
	},
    "win": {
      "target": "portable"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "angular-emojify": "^0.2.0",
    "core-js": "^2.4.1",
    "fstream": "^1.0.11",
    "ncp": "^2.0.0",
    "rxjs": "^5.1.0",
    "unzip": "^0.1.11",
    "uuid": "^3.0.1",
    "xml2js": "^0.4.17",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.0.4",
    "@angular/compiler-cli": "^4.0.0",
    "@types/electron": "^1.4.38",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "autoprefixer": "^6.5.3",
    "better-npm-run": "0.0.15",
    "browser-sync": "^2.18.12",
    "browser-sync-webpack-plugin": "^1.1.4",
    "codelyzer": "~2.0.0",
    "concurrently": "^3.4.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.1",
    "cssnano": "^3.10.0",
    "electron": "^1.6.8",
    "electron-builder": "^18.3.5",
    "exports-loader": "^0.6.3",
    "file-loader": "^0.10.0",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-zip": "^4.0.0",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "json-loader": "^0.5.4",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-sourcemap-loader": "^0.3.7",
    "less-loader": "^4.0.2",
    "postcss-loader": "^1.3.3",
    "postcss-url": "^5.1.2",
    "protractor": "~5.1.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.3",
    "script-loader": "^0.7.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "stylus-loader": "^3.0.1",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typescript": "~2.2.0",
    "url-loader": "^0.5.7",
    "webpack": "~2.2.0",
    "webpack-dev-server": "~2.4.2"
  }
}
